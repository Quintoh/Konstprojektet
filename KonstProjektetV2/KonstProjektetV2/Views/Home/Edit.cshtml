@model KonstProjektetV2.Models.TableInsertModel

@{
    ViewBag.Title = "Edit";
}

@using (Html.BeginForm("Edit", "Home", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="row">
        <div class="col-lg-6">
            <div class="form-group">
                @Html.LabelFor(x => x.Author)
                @Html.HiddenFor(x => x.Author)
                @Html.TextBoxFor(x => x.Author, new { disabled = "disabled" })
            </div>
            <div class="form-group">
                @Html.LabelFor(x => x.Title)
                @Html.HiddenFor(x => x.Title)
                @Html.TextBoxFor(x => x.Title, new { disabled = "disabled" })
            </div>
            <div class="form-group">
                @Html.LabelFor(x => x.Description)
                @Html.TextBoxFor(x => x.Description)
            </div>
            <div class="form-group">
                @Html.LabelFor(x => x.Type)
                @Html.TextBoxFor(x => x.Type)
            </div>
            <div class="form-group">
                @Html.LabelFor(x => x.Location)
                @Html.TextBoxFor(x => x.Location)
            </div>
            <div class="form-group">
                <img src="http://t4boys2016.blob.core.windows.net/fouramigos/@Model.FileName" />
                @Html.LabelFor(x => x.File)
                @Html.TextBoxFor(m => m.File, new
                {
                    type = "file",
                })
            </div>

            <input type="submit" value="Spara" />
        </div>
    </div>
}


