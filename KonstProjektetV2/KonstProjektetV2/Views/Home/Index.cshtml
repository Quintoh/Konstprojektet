
@{
    ViewBag.Title = "Index";
}

<script type="text/javascript">

    $('.carousel').carousel({
        interval: 6000,
        pause: "false"
    });
 
</script>

<style type="text/css">
h3 {
  display: inline-block;
  padding: 10px;
  background: #B9121B;
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
}

.p {
  text-align: center;
  padding-top: 40px;
  font-size: 13px;
}

.full-screen {
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}

</style>


